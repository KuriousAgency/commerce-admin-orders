{
	"name": "kuriousagency/commerce-admin-orders",
	"description": "Create a new commerce order from the admin",
	"type": "craft-plugin",
	"version": "2.1.1",
	"keywords": [
		"craft",
		"cms",
		"craftcms",
		"craft-plugin",
		"commerce admin orders"
	],
	"support": {
		"docs": "https://github.com/KuriousAgency/commerce-admin-orders/blob/master/README.md",
		"issues": "https://github.com/KuriousAgency/commerce-admin-orders/issues"
	},
	"license": "MIT",
	"authors": [
		{
			"name": "Kurious Agency",
			"homepage": "https://kurious.agency"
		}
	],
	"require": {
